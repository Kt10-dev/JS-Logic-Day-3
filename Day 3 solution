        // me sirf ek baar jaa skte ho fir batana hai konsa Switch se 
        // connection hai bulb kaa. 
  
//  Q.3 First Recurring Character 
        // strng = "acbbad"
        // task = pehla voh character retuen karna hai dubara repeat huyaho
        
        let strng = "acabbad"
        let count = {}
        function Frsercurr(strng,count){
          for(let i = 0; i<strng.length;i++){
            let chr=strng[i]
            if (count[chr]) {
              return chr
              
            }else{
              count[chr]=1
            }
          }
        }
        
        console.log(Frsercurr(strng,count))
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
