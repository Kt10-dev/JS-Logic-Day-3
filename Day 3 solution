// Q.1 Missing Number  [1,2,4,5]
    // Task = Hume MissingNumber Find Karna hai
    
  let arr = [1,2,4,5]
  let n = 5
  
  function MssisngNumFnd(arr,n){
    let xor1 = 0
    let xor2 = 0
    
    for(let i = 1; i<=n;i++){
      xor1 = xor1 ^ i
    }
    for(let i = 0;i <arr.length;i++){
      xor2 = xor2 ^ arr[i]
    }
    return xor1 ^ xor2
  }
  
  console.log(MssisngNumFnd(arr,n))
    
    
    
    
    
    
    
