// (Logic samajh ke answer do.)

 
// Question 3. 
// Ek string diya hai:

// "apple banana apple mango banana apple"

let strrig = "apple banana apple mango banana apple"
let count ={}

let words = strrig.split(" ")



function StrnfFind(count,words){
  
  for(let word of words){
    
    if(count[word]){
      count[word] +=1
    }else{
      count[word] = 1
    }
  }
}

StrnfFind(count,words)

console.log(count)










