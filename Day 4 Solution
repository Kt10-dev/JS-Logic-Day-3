// Question 1 .
// Array Pair Sum
// Ek array diya hai:
// arr = [2, 7, 11, 15]
// Target diya hai:
// target = 9
// ğŸ‘‰ Kaunsa pair sum karke 9 banata hai?


let arr = [ 11, 15,2, 7]
let target = 9

function AddSum(arr,target){
  for (var i = 0; i < arr.length; i++) {
    let value = arr[i]
    for(let j = i +1 ; j <arr.length; j++ ){
      let valuu1= arr[j]
      
      if ([value + valuu1] == target) {
        return [value ,valuu1]
      }
    }
    
  }
}

console.log(AddSum(arr,target))



